{
  "name": "import-chain",
  "version": "0.0.1",
  "description": "import chain",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup ./src/index.ts --watch src --format esm",
    "build": "tsup ./src/index.ts --clean --format esm",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --fix",
    "prepublishOnly": "pnpm build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sishenhei7/import-chain.git"
  },
  "homepage": "https://github.com/sishenhei7/import-chain#readme",
  "bugs": {
    "url": "https://github.com/sishenhei7/import-chain/issues"
  },
  "author": "yangzhou",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/node": "^16.6.2",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.2",
    "nodemon": "^2.0.12",
    "prettier": "^2.8.4",
    "tsup": "^6.6.3"
  },
  "dependencies": {
    "typescript": "^4.9.5"
  }
}
